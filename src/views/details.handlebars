<div class="container">

  <div class="card-view">
    {{#each tabela}}

    <a href="#modal-details">
      <div class="card-details">
        <span>{{this.nomeFila}}</span>
      </div>
    </a>
  </div>
  {{/each}}
</div>

<div id="modal-details" class="modal">
  <div class="modal-content">

    <h1>{{this.nomeFila}}</h1>

    <form method="post" action="/settings/add/printer">

      <div class="form-group">
        <b>IP</b>
        <span>Ip da fila</span>
      </div>

      <div class="form-group">
        <b>Fabricante</b>
        <span>Fabricante da impressora</span>
      </div>

      <div class="form-group">
        <b>Modelo</b>
        <span>Modelo da fila</span>
      </div>

      <div class="form-group">
        <b>Serial</b>
        <span>Serial da fila</span>
      </div>

      <div class="form-group">
        <b>Contador</b>
        <span>Contador da fila</span>
      </div>

      <div class="form-group">
        <b>Toner</b>
        <span>Toner da fila</span>
      </div>

      <div class="form-group">
        <b>Kit de manutenção</b>
        <span>Kit de manutenção da fila</span>
      </div>

      <div class="form-group">
        <b>Unidade de imagem</b>
        <span>Unidade de imagem da fila</span>
      </div>

    </form>

    <a href="#" class="modal-close">
      <i class="fa-solid fa-square-xmark fa-fw fa-2x"></i>
    </a>

  </div>
</div>